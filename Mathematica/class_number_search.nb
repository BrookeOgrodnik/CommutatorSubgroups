Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Given", " ", "a", " ", "matrix"}], ",", " ", "g", ",", " ", 
    RowBox[{"given", " ", "its", " ", "continued", " ", "fraction", " ", 
     RowBox[{"represenatation", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"cyclicrepofmatrix", "[", "g_", "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"makeit", "=", 
      RowBox[{"ContinuedFraction", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"g", "[", 
               RowBox[{"[", 
                RowBox[{"2", ",", "2"}], "]"}], "]"}], "-", 
              RowBox[{"g", "[", 
               RowBox[{"[", 
                RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ")"}]}], "+", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"g", "[", 
                 RowBox[{"[", 
                  RowBox[{"2", ",", "2"}], "]"}], "]"}], "-", 
                RowBox[{"g", "[", 
                 RowBox[{"[", 
                  RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ")"}], "^", "2"}], 
             "+", 
             RowBox[{"4", 
              RowBox[{"g", "[", 
               RowBox[{"[", 
                RowBox[{"1", ",", "2"}], "]"}], "]"}], "*", 
              RowBox[{"g", "[", 
               RowBox[{"[", 
                RowBox[{"2", ",", "1"}], "]"}], "]"}]}]}], "]"}]}], ")"}], 
         "/", "2"}], "/", 
        RowBox[{"g", "[", 
         RowBox[{"[", 
          RowBox[{"2", ",", "1"}], "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cyclicrep", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"makeit", "[", 
          RowBox[{"[", "1", "]"}], "]"}], ",", 
         RowBox[{
          RowBox[{"makeit", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "[", 
          RowBox[{"[", 
           RowBox[{"1", ";;", 
            RowBox[{
             RowBox[{"Length", "[", 
              RowBox[{"makeit", "[", 
               RowBox[{"[", "2", "]"}], "]"}], "]"}], "-", "1"}]}], "]"}], 
          "]"}]}], "}"}], "]"}]}]}], ")"}]}]}]], "Input",
