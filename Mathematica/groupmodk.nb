Cell[BoxData[
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"x1", "=", "0"}], ",", " ", 
   RowBox[{"x1", "<", "3"}], ",", " ", 
   RowBox[{"x1", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"x2", "=", "0"}], ",", 
     RowBox[{"x2", "<", "3"}], ",", 
     RowBox[{"x2", "++"}], ",", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"x3", "=", "0"}], ",", 
       RowBox[{"x3", "<", "3"}], ",", 
       RowBox[{"x3", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"x4", "=", "0"}], ",", 
         RowBox[{"x4", "<", "3"}], ",", 
         RowBox[{"x4", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"x1", "\[NotEqual]", "0"}], "||", 
            RowBox[{"x2", "\[NotEqual]", "0"}], "||", 
            RowBox[{"x3", "\[NotEqual]", "0"}], "||", 
            RowBox[{"x4", "\[NotEqual]", "0"}]}], ",", "\[IndentingNewLine]", 
           
           RowBox[{
            RowBox[{"temp", "=", 
             RowBox[{"DeleteDuplicates", "[", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{"k", "/.", 
                    RowBox[{"a1", "\[Rule]", "x1"}]}], "/.", 
                   RowBox[{"b1", "\[Rule]", "x2"}]}], "/.", 
                  RowBox[{"c1", "\[Rule]", "x3"}]}], "/.", 
                 RowBox[{"d1", "\[Rule]", "x4"}]}], ",", "3"}], "]"}], 
              "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Length", "[", "temp", "]"}], "\[NotEqual]", "27"}], 
              ",", 
              RowBox[{"Print", "[", 
               RowBox[{"{", 
                RowBox[{"x1", ",", "x2", ",", "x3", ",", "x4"}], "}"}], 
               "]"}]}], "]"}]}]}], "]"}]}], "]"}]}], "]"}]}], "]"}]}], 
  "]"}]], "Input",
