Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "We", " ", "just", " ", "find", " ", "all", " ", "such", " ", "matrices", 
    " ", "and", " ", "look", " ", "at", " ", "their", " ", "traces", " ", 
    "to", " ", "see", " ", "that", " ", "it", " ", "matches", " ", "the", " ",
     "statement", " ", "of", " ", "case", " ", "2.", "  ", "I", " ", 
    "recommend", " ", "running", " ", "this", " ", "in", " ", "Python", " ", 
    "or", " ", 
    RowBox[{"C", "++"}], " ", "since", " ", "the", " ", "Mathematica", " ", 
    "version", " ", "take", " ", "SO", " ", 
    RowBox[{"long", "."}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"mod2p8", "=", 
     RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"Monitor", "[", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"x1", "=", "0"}], ",", " ", 
       RowBox[{"x1", "<", 
        RowBox[{"2", "^", "8"}]}], ",", " ", 
       RowBox[{"x1", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"x2", "=", "0"}], ",", 
         RowBox[{"x2", "<", 
          RowBox[{"2", "^", "8"}]}], ",", 
         RowBox[{"x2", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"x3", "=", "0"}], ",", 
           RowBox[{"x3", "<", 
            RowBox[{"2", "^", "8"}]}], ",", 
           RowBox[{"x3", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"x4", "=", "0"}], ",", 
             RowBox[{"x4", "<", 
              RowBox[{"2", "^", "8"}]}], ",", 
             RowBox[{"x4", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"Mod", "[", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"x1", "*", "x4"}], "-", 
                    RowBox[{"x2", "*", "x3"}]}], ",", 
                   RowBox[{"2", "^", "8"}]}], "]"}], "\[Equal]", "1"}], " ", "&&",
                 " ", 
                RowBox[{"MemberQ", "[", 
                 RowBox[{"g1", ",", 
                  RowBox[{"Mod", "[", 
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"x1", ",", "x2"}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"x3", ",", "x4"}], "}"}]}], "}"}], ",", "16"}], 
                   "]"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
               RowBox[{"AppendTo", "[", 
                RowBox[{"mod2p8", ",", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"{", 
                    RowBox[{"x1", ",", "x2"}], "}"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"x3", ",", "x4"}], "}"}]}], "}"}]}], "]"}]}], 
              "]"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x1", ",", "x2", ",", "x3", ",", "x4"}], "}"}]}], 
    "]"}]}]}]], "Input",
