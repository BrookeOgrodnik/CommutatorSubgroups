Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "We", " ", "find", " ", "all", " ", "matrices", " ", "in", " ", "Gamma", 
    RowBox[{
     RowBox[{"(", "2", ")"}], "'"}], " ", "modulo", " ", "9", " ", "using", 
    " ", "Theorem", " ", "3.0", ".1", " ", "and", " ", "look", " ", "at", " ",
     "their", " ", "trace", " ", "set"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"Monitor", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"mod3p2", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"x1", "=", "0"}], ",", " ", 
       RowBox[{"x1", "<", "9"}], ",", " ", 
       RowBox[{"x1", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"x2", "=", "0"}], ",", 
         RowBox[{"x2", "<", "9"}], ",", 
         RowBox[{"x2", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"x3", "=", "0"}], ",", 
           RowBox[{"x3", "<", "9"}], ",", 
           RowBox[{"x3", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"x4", "=", "0"}], ",", 
             RowBox[{"x4", "<", "9"}], ",", 
             RowBox[{"x4", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"Mod", "[", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"x1", "*", "x4"}], "-", 
                    RowBox[{"x2", "*", "x3"}]}], ",", "9"}], "]"}], 
                 "\[Equal]", "1"}], " ", "&&", " ", 
                RowBox[{"MemberQ", "[", 
                 RowBox[{"g2", ",", 
                  RowBox[{"Mod", "[", 
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"x1", ",", "x2"}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"x3", ",", "x4"}], "}"}]}], "}"}], ",", "3"}], 
                   "]"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
               RowBox[{"AppendTo", "[", 
                RowBox[{"mod3p2", ",", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"{", 
                    RowBox[{"x1", ",", "x2"}], "}"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"x3", ",", "x4"}], "}"}]}], "}"}]}], "]"}]}], 
              "]"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x1", ",", "x2", ",", "x3", ",", "x4"}], "}"}]}], 
   "]"}]}]], "Input",
